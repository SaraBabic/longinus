{% set longinus_block = 'view-' ~ view.storage.id()|clean_class ~ '-' ~ view.current_display|clean_class %}
{% set longinus_element = longinus_block ~ '__list' %}
{% if attributes|length -%}
  <div{{ attributes.addClass(longinus_element ~ '-container') }}>
{% endif %}
  {% if title %}
    <h3 class="{{ longinus_element }}-title">{{ title }}</h3>
  {% endif %}
  {% block content %}
  <{{ list.type }}{% if list.attributes %}{{ list.attributes.addClass(longinus_element) }}{% else %} class="{{ longinus_element }}"{% endif %}>

    {% for row in rows %}
      <li{{ row.attributes.addClass(longinus_element ~ '-item') }}>{{ row.content }}</li>
    {% endfor %}

  </{{ list.type }}>
  {% endblock %}
{% if attributes|length -%}
  </div>
{% endif %}
